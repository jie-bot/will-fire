Event.total > 0) {\n          progressEvent.percent = progressEvent.loaded / progressEvent.total * 100;\n        }\n        option.onProgress(progressEvent);\n      };\n    }\n\n    return new Promise((resolve, reject) => {\n      axios\n        .post(url, param, config)\n        .then(res => {\n          resolve(res.data);\n        })\n        .catch(err => {\n          reject(err);\n        });\n    });\n  },\n\n  uploadQiniu(url, param) {\n    let config = {\n      headers: {\"Content-Type\": \"multipart/form-data\"},\n      timeout: 60000\n    };\n\n    return new Promise((resolve, reject) => {\n      axios\n        .post(url, param, config)\n        .then(res => {\n          resolve(res.data);\n        })\n        .catch(err => {\n          reject(err);\n        });\n    });\n  }\n}\n"],"mappings":";;AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,QAAP,MAAqB,YAArB,C,CACA;;AACA,OAAOC,EAAP,MAAe,IAAf;AAEA,OAAOC,KAAP,MAAkB,UAAlB;AAGAH,KAAK,CAACI,QAAN,CAAeC,OAAf,GAAyBJ,QAAQ,CAACI,OAAlC,C,CAGA;;AACAL,KAAK,CAACM,YAAN,CAAmBC,OAAnB,CAA2BC,GAA3B,CAA+B,UAAUC,MAAV,EAAkB;EAC/C;EACA,OAAOA,MAAP;AACD,CAHD,EAGG,UAAUC,KAAV,EAAiB;EAClB;EACA,OAAOC,OAAO,CAACC,MAAR,CAAeF,KAAf,CAAP;AACD,CAND,E,CAQA;;AACAV,KAAK,CAACM,YAAN,CAAmBO,QAAnB,CAA4BL,GAA5B,CAAgC,UAAUK,QAAV,EAAoB;EAClD,IAAIA,QAAQ,CAACC,IAAT,KAAkB,IAAlB,IAA0BD,QAAQ,CAACC,IAAT,CAAcC,cAAd,CAA6B,MAA7B,CAA1B,IAAkEF,QAAQ,CAACC,IAAT,CAAcE,IAAd,KAAuB,GAA7F,EAAkG;IAChG,IAAIH,QAAQ,CAACC,IAAT,CAAcE,IAAd,KAAuB,GAA3B,EAAgC;MAC9Bb,KAAK,CAACc,MAAN,CAAa,iBAAb,EAAgC,EAAhC;MACAC,YAAY,CAACC,UAAb,CAAwB,WAAxB;MACAhB,KAAK,CAACc,MAAN,CAAa,kBAAb,EAAiC,EAAjC;MACAC,YAAY,CAACC,UAAb,CAAwB,YAAxB;MACAC,MAAM,CAACC,QAAP,CAAgBC,IAAhB,GAAuBrB,QAAQ,CAACsB,MAAT,GAAkB,OAAzC;IACD;;IACD,OAAOZ,OAAO,CAACC,MAAR,CAAe,IAAIY,KAAJ,CAAUX,QAAQ,CAACC,IAAT,CAAcW,OAAxB,CAAf,CAAP;EACD,CATD,MASO;IACL,OAAOZ,QAAP;EACD;AACF,CAbD,EAaG,UAAUH,KAAV,EAAiB;EAClB;EACA,OAAOC,OAAO,CAACC,MAAR,CAAeF,KAAf,CAAP;AACD,CAhBD,E,CAkBA;AACA;;AAGA,eAAe;EACbgB,IADa,gBACRC,GADQ,EACwC;IAAA,IAA3CC,MAA2C,uEAAlC,EAAk